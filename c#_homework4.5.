Izveidot console aplikāciju, kura lietotājam paprasīs, cik reizes ir jāatkārto for cikls un izvadīs "Hello World!"


﻿using System;

namespace Uzdevums4._5
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Ievadiet skaitli cik reizes izprintet Hello World!");
            string t = Console.ReadLine();
            int y = Convert.ToInt32(t);
            int n = 0;

            while (n < y)
            {
                Console.WriteLine("Hello World!");
                n++;
            }
        }
    }
}
